{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "rules": {
    "variable-name": [
      true,
      "allow-leading-underscore",
      "allow-pascal-case"
    ],
    "no-console": {
      "severity": "warning",
      "options": [
          "debug",
          "info",
          "log",
          "time",
          "timeEnd",
          "trace"
      ]
    }
  },
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  }
}
